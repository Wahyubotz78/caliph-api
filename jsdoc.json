{
    "source": {
        "include": ["lib"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "readme": "./README.md"
    }
}
